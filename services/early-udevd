# A scripted service that runs early udevd that is not supervised, later
# replaced with a real supervised service once everything else is up

type         = scripted
command      = /usr/bin/udevd --daemon
stop-command = /usr/bin/udevadm control -e
logfile      = /run/dinit-udevd.log
restart      = false
depends-on   = early-filesystems

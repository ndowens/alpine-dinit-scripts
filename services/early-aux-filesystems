# Auxiliary (non-root) filesystems

type          = scripted
command       = /usr/libexec/dinit/early/aux-filesystems.sh
restart       = false
logfile       = /var/log/dinit-filesystems.log
options       = start-interruptible
start-timeout = 1200 # 20 minutes
depends-on    = early-udevd
depends-on    = early-root-rw
waits-for     = early-aux-fsck
waits-for     = early-udev-settle

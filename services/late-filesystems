# Filesystems which can be mounted after login is enabled
# This runs after all early oneshots are run

type          = scripted
command       = /etc/dinit.d/scripts/late-filesystems.sh
restart       = false
logfile       = /var/log/dinit-late-filesystems.log
options       = start-interruptible
start-timeout = 0 # unlimited
depends-on    = early-rcboot

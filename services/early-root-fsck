# Check the root filesystem, interruptible with ^C

type          = scripted
command       = /usr/libexec/dinit/early/root-fsck.sh
restart       = false
options       = starts-on-console pass-cs-fd start-interruptible skippable
start-timeout = 0 # unlimited
depends-on    = early-filesystems
depends-on    = early-udevd
waits-for     = early-udev-trigger
